  "is this update even good ",
  "Smash down Chez and Doktem! They are low-ðŸ”‘ buns bro.",
  "Loading...",
  "Smash down Chez and Doktem! They are low-ðŸ”‘ buns bro.",
  "T!tans way â€” games for when teachers look away."
];

document.getElementById("quote").textContent =
  quotes[Math.floor(Math.random() * quotes.length)];

setTimeout(() => {
  document.getElementById("loading").style.display = "none";
  document.getElementById("app").style.display = "block";
}, 693);

function filterImages() {
  const q = document.getElementById("search").value.toLowerCase();
  const items = document.querySelectorAll(".item");
  const noResults = document.getElementById("no-results");
  let matches = 0;

  items.forEach(item => {
    const title = item.dataset.title.toLowerCase();
    if (title.includes(q)) {
      item.style.display = "block";
      matches++;
    } else {
      item.style.display = "none";
    }
  });

  noResults.style.display = matches === 0 ? "block" : "none";
}

document.getElementById("close-ad").addEventListener("click", function (e) {
    e.stopPropagation();
    document.getElementById("notification-ad").style.display = "none";
  });
</script>

